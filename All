5194  /***
5195  Creating variable buckets : 0-p25, p25-p50, p50-p75, p75-p100
5196  EmployeeNumber :    <=500, 500-1000,    1000-1500,  >1500
5197  HourlyRate :        <=50,   50-65,      65-85,      >85
5198  Age:                <=30,   31-35,      36-45,      >45
5199  Income:             <=3000, 3000-5000,  5000-8500,  >8500
5200  DistfromHome:       <=2,    3-7,        8-14,       >=15
5201
5202  Job level is already categorical from 1 to 5
5203  Job Satisfaction is also categorical from 1 to 4
5204  ***/
5205
5206  /**** Frequency analysis of categorical variables to understand distribution ***/
5207  proc freq data =in.Modified_ferma1;
5208  tables JobLevel JobSatisfaction environment_satisfaction job_involvement Gender JobRole
5208! MaritalStatus / norow nocol nopct;
5209  title 'Frequency of categorical variables';
5210  run;

