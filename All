5515  /* -----------------------------------Combined
5515! Model------------------------------------------------ */
5516  PROC PHREG DATA = in.Modified_ferma2;
5517      WHERE YearsAtCompany > 1;
5518      CLASS BusinessTravel Department EducationField  JobRole MaritalStatus  AgeBucket
5518! IncomeBucket Gender(ref = 'Female')
5519       OverTime(ref = 'No') DistanceFromHomeBucket EmployeeType ;
5520      MODEL YearsAtCompany*Type(0) = Age  DailyRate   DistanceFromHome Education
5520! EnvironmentSatisfaction HourlyRate
5521      JobInvolvement  JobLevel JobSatisfaction MonthlyIncome  MonthlyRate NumCompaniesWorked
5521! PercentSalaryHike
5522      PerformanceRating RelationshipSatisfaction StockOptionLevel TotalWorkingYears
5522! TrainingTimesLastYear   WorkLifeBalance
5523      YearsInCurrentRole YearsSinceLastPromotion YearsWithCurrManager YearsBonusRewarded_cal
5523! BonusRewardRatio_cal HoursWorkedDaily
5524      PreviousYearBonus HoursWokedMonthly BusinessTravel Department EducationField  JobRole
5524! MaritalStatus  AgeBucket
5525      IncomeBucket Gender OverTime DistanceFromHomeBucket EmployeeType / TIES=EFRON
5525! SELECTION=backward SLSTAY=0.05;
5526      ARRAY bonus_(*) bonus_1-bonus_40;
5527      PreviousYearBonus = bonus_[YearsAtCompany - 1];
5528          YearsBonusRewarded_cal = sum(of bonus_1 - bonus_40);
5529          BonusRewardRatio_cal = YearsBonusRewarded_cal/YearsAtCompany;
5530  RUN;

NOTE: Convergence criterion (GCONV=1E-8) satisfied for the model in Step 0.
