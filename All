5765
5766  /* Model for Type 4 Turnover (Fired) */
5767  PROC PHREG DATA = in.Modified_ferma2;
5768      WHERE YearsAtCompany > 1;
5769      CLASS BusinessTravel Department EducationField  JobRole MaritalStatus  AgeBucket
5769! IncomeBucket Gender(ref = 'Female')
5770       OverTime(ref = 'No') DistanceFromHomeBucket EmployeeType ;
5771      MODEL YearsAtCompany*Type(0,1,2,3) = Age  DailyRate DistanceFromHome Education
5771! EnvironmentSatisfaction HourlyRate
5772      JobInvolvement  JobLevel JobSatisfaction MonthlyIncome  MonthlyRate NumCompaniesWorked
5772! PercentSalaryHike
5773      PerformanceRating RelationshipSatisfaction StockOptionLevel TotalWorkingYears
5773! TrainingTimesLastYear   WorkLifeBalance
5774      YearsInCurrentRole YearsSinceLastPromotion YearsWithCurrManager YearsBonusRewarded_cal
5774! BonusRewardRatio_cal HoursWorkedDaily
5775      PreviousYearBonus HoursWokedMonthly BusinessTravel Department EducationField  JobRole
5775! MaritalStatus  AgeBucket
5776      IncomeBucket Gender OverTime DistanceFromHomeBucket EmployeeType / TIES=EFRON
5776! SELECTION=backward SLSTAY=0.05;
5777      ARRAY bonus_(*) bonus_1-bonus_40;
5778      PreviousYearBonus = bonus_[YearsAtCompany - 1];
5779          YearsBonusRewarded_cal = sum(of bonus_1 - bonus_40);
5780          BonusRewardRatio_cal = YearsBonusRewarded_cal/YearsAtCompany;
5781  RUN;

