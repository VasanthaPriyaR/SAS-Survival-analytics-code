5493  DATA LogRatioTest_DoWeNeedThreeModels;
5494      Nested=2091.492;
5495      Fired=91.122;
5496      Voluntary=828.949;
5497      Involuntary=44.793;
5498      Retired=311.036;
5499      Total=Fired+Voluntary+Involuntary+Retired;
5500      Diff=Nested - Total;
5501      P_value=1 - probchi(Diff, 117);
5502      *39*4 coef. in 4 models - 39 coef. in nested;
5503  RUN;

