      real time           0.02 seconds
      cpu time            0.01 seconds


5211
5212  data in.Modified_ferma1;
5213  set in.Modified_ferma1;
5214
5215  ID='               ';
5216  if EmployeeNumber<=500 then ID='a. <=500';
5217  else if EmployeeNumber<=1000 then ID='b. 500-1000';
5218  else if EmployeeNumber<=1500 then ID='c. 1000-1500';
5219  else ID='d. >1500';
5220
5221  Hourly_rate='             ';
5222  if HourlyRate<=50 then Hourly_rate='a. <=50';
5223  else if HourlyRate<=65 then Hourly_rate='b. 50-65';
5224  else if HourlyRate<=85 then Hourly_rate='c. 65-85';
5225  else Hourly_rate='d. >85';
5226
5227  AgeBucket='             ';
5228  if Age<=30 then AgeBucket='a. <=30';
5229  else if Age<=35 then AgeBucket='b. 31-35';
5230  else if Age<=45 then AgeBucket='c. 36-45';
5231  else AgeBucket='d. >=46';
5232
5233  IncomeBucket='             ';
5234  if MonthlyIncome<=3000 then IncomeBucket='a. <=3K';
5235  else if MonthlyIncome<=5000 then IncomeBucket='b. 3K-5K';
5236  else if MonthlyIncome<=8500 then IncomeBucket='c. 5K-8.5K';
5237  else IncomeBucket='d. >=8.5K';
5238
5239  DistanceFromHomeBucket='             ';
5240  if DistanceFromHome<=2 then DistanceFromHomeBucket='a. <=2';
5241  else if DistanceFromHome<=7 then DistanceFromHomeBucket='b. 3-7';
5242  else if DistanceFromHome<=14 then DistanceFromHomeBucket='c. 8-14';
5243  else DistanceFromHomeBucket='d. >=15';
5244
5245  run;

