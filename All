5658  /* Adding Interaction variables and STRATA statement*/
5659  PROC PHREG DATA = in.Modified_ferma2;
5660      WHERE YearsAtCompany > 1;
5661      CLASS BusinessTravel Department EducationField  JobRole MaritalStatus  AgeBucket
5661! IncomeBucket Gender(ref = 'Female')
5662       OverTime(ref = 'No') DistanceFromHomeBucket EmployeeType ;
5663      MODEL YearsAtCompany*Type(0,2,3,4) = Age  DailyRate DistanceFromHome Education
5663! EnvironmentSatisfaction HourlyRate
5664      JobInvolvement  JobLevel JobSatisfaction MonthlyIncome  MonthlyRate NumCompaniesWorked
5664! PercentSalaryHike
5665      PerformanceRating RelationshipSatisfaction StockOptionLevel TotalWorkingYears
5665! TrainingTimesLastYear   WorkLifeBalance
5666      YearsInCurrentRole YearsSinceLastPromotion YearsWithCurrManager YearsBonusRewarded_cal
5666! BonusRewardRatio_cal HoursWorkedDaily
5667      PreviousYearBonus HoursWokedMonthly BusinessTravel Department EducationField  JobRole
5667! MaritalStatus  AgeBucket
5668      IncomeBucket Gender OverTime DistanceFromHomeBucket EmployeeType Int_NumCompaniesWorked
5668! Int_YearsWithCurrManager
5669       / TIES=EFRON SELECTION=backward SLSTAY=0.05;
5670      ARRAY bonus_(*) bonus_1-bonus_40;
5671      PreviousYearBonus = bonus_[YearsAtCompany - 1];
5672          YearsBonusRewarded_cal = sum(of bonus_1 - bonus_40);
5673          BonusRewardRatio_cal = YearsBonusRewarded_cal/YearsAtCompany;
5674      Int_NumCompaniesWorked = NumCompaniesWorked * YearsAtCompany;
5675      Int_YearsWithCurrManager = YearsWithCurrManager * YearsAtCompany;
5676      STRATA BusinessTravel;
5677  RUN;

