5755       OverTime(ref = 'No') DistanceFromHomeBucket EmployeeType ;
5756      MODEL YearsAtCompany*Type(0,2,1,4) = Age  DailyRate DistanceFromHome Education
5756! EnvironmentSatisfaction HourlyRate
5757      JobInvolvement  JobLevel JobSatisfaction MonthlyIncome  MonthlyRate NumCompaniesWorked
5757! PercentSalaryHike
5758      PerformanceRating RelationshipSatisfaction StockOptionLevel TotalWorkingYears
5758! TrainingTimesLastYear   WorkLifeBalance
5759      YearsInCurrentRole YearsSinceLastPromotion YearsWithCurrManager HoursWorkedDaily
5760      HoursWokedMonthly BusinessTravel Department EducationField  JobRole MaritalStatus
5760! AgeBucket
5761      IncomeBucket Gender OverTime DistanceFromHomeBucket EmployeeType / TIES=EFRON
5761! SELECTION=backward SLSTAY=0.05;
5762      ASSESS PH / RESAMPLE ;
5763  RUN;

