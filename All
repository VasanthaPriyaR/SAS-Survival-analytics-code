4747  /***** Exploratory Data Analysis ********************/
4748
4749


4750  data in.modified_ferma1;
4751  set in.modified_ferma;
4752
4753  HoursWorkedDaily = DailyRate/HourlyRate;
4754  HoursWokedMonthly = MonthlyRate/DailyRate;
4755
4756  /*** Correcting Turnover variable using Type ***/
4757
4758  if Type>0 then Turnover='Yes';
4759  else Turnover='No';
4760
4761  /*** Creating Employee Experience category ***/
4762
4763  IF YearsAtCompany <= 5 THEN EmployeeType = 'Young';
4764  ELSE EmployeeType = 'Experienced';
4765
4766  YearsBonusRewarded = SUM ( bonus_1 , bonus_2 , bonus_3 , bonus_4 , bonus_5 ,
4767      bonus_6 , bonus_7 , bonus_8 , bonus_9   , bonus_10 ,
4768      bonus_11 , bonus_12 , bonus_13  , bonus_14 , bonus_15 , bonus_16 ,
4769      bonus_17 , bonus_18 , bonus_19 , bonus_20 , bonus_21 , bonus_22 ,
4770      bonus_23 , bonus_24 , bonus_25 , bonus_26 , bonus_27 , bonus_28 ,
4771      bonus_29 , bonus_30 , bonus_31 , bonus_32 , bonus_33 , bonus_34 ,
4772      bonus_35 , bonus_36 , bonus_37 , bonus_38 , bonus_39 , bonus_40);
4773
4774  /*** Calculating Bonus Ratio per year with company ***/
4775  BonusRewardRatio = ROUND ((YearsBonusRewarded/YearsAtCompany),0.1) ;
4776
4777
4778  /**** Mapping of variables ***/
4779
4780  if environmentsatisfaction=4 then environment_satisfaction='a.Very High';
4781  else if environmentsatisfaction=3 then environment_satisfaction='b.High';
4782  else if environmentsatisfaction=2 then environment_satisfaction='c.Medium';
4783  else if environmentsatisfaction=1 then environment_satisfaction='d.Low';
4784
4785  if jobinvolvement=4 then job_involvement='a.Very High';
4786  else if jobinvolvement=3 then job_involvement='b.High';
4787  else if jobinvolvement=2 then job_involvement='c.Medium';
4788  else if jobinvolvement=1 then job_involvement='d.Low';
4789
4790  attrition_rsn='                ';
4791  if type=0 then attrition_rsn='a. No_Turnover';
4792  else if type=1 then attrition_rsn='b. Retirement';
4793  else if type=2 then attrition_rsn='c. Voluntary';
4794  else if type=3 then attrition_rsn='d. Personal';
4795  else if type=4 then attrition_rsn='e. Fired';
4796
4797  run;

NOTE: Missing values were generated as a result of performing an operation on missing values.
      Each place is given by: (Number of times) at (Line):(Column).
      215 at 4766:22   215 at 4775:20   215 at 4775:46
NOTE: There were 1470 observations read from the data set IN.MODIFIED_FERMA.
